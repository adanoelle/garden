{
  "name": "@garden/types",
  "version": "0.1.0",
  "description": "TypeScript types for Garden - generated from Rust",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  }
}
